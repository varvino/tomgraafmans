{% extends "base.twig" %}

{% block content %}
	{% for post in posts %}
		<div class="post">
			<h1 class="post__title">{{post.title}}</h1>
			<p>Heeft mijn portfolio uw interesse gewekt? Neem contact met mij op!</p>
			<div class="post__content">{{post.content}}</div>
		</div>
	{% endfor %}
{% endblock %}
